<!DOCTYPE html>

  <head>
    <script src="https://code.jquery.com/jquery-3.4.1.js" integrity="sha256-WpOohJOqMqqyKL9FccASB9O0KwACQJpFTUBLTYOVvVU=" crossorigin="anonymous"></script>
<script>  $(document).ready(function(){     $("#itWorks").html("jQuery works!");}); </script>
<script> $(document).ready(function(){
		$("#clickme").click(function(){
			$("#clickme").css("color", "#F00");
			$("#clickme").html("The text has changed to red because you clicked me");
		});}); </script>

<script> $(document).ready(function(){
	$("#mytoggle").click(function(){
		var mystate = $("#mytoggle").html();
		if (mystate === "TRUE")
		{
			$("#mytoggle").html("FALSE");
		}
		else
		{
			$("#mytoggle").html("TRUE");
		}
	});
}); </script>

    <title>jQuery test</title>
  </head>
  <body> 
        <h2 id="clickme">Click me!</h2>
<h1 id="mytoggle">TRUE</h1>

  </body>

</html>
